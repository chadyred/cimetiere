{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ConcessionaireArchive list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Titre</th>
                <th>Nomprenom</th>
                <th>Nomjf</th>
                <th>Cp</th>
                <th>Ville</th>
                <th>Adresse1</th>
                <th>Adresse2</th>
                <th>Codegestionconcession</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('concessionairearchive_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.titre }}</td>
                <td>{{ entity.nomPrenom }}</td>
                <td>{{ entity.nomJF }}</td>
                <td>{{ entity.cp }}</td>
                <td>{{ entity.ville }}</td>
                <td>{{ entity.adresse1 }}</td>
                <td>{{ entity.adresse2 }}</td>
                <td>{{ entity.codeGestionConcession }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('concessionairearchive_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('concessionairearchive_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('concessionairearchive_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}

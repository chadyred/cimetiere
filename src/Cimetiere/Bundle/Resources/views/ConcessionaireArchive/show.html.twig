{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ConcessionaireArchive</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ entity.titre }}</td>
            </tr>
            <tr>
                <th>Nomprenom</th>
                <td>{{ entity.nomPrenom }}</td>
            </tr>
            <tr>
                <th>Nomjf</th>
                <td>{{ entity.nomJF }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ entity.cp }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ entity.ville }}</td>
            </tr>
            <tr>
                <th>Adresse1</th>
                <td>{{ entity.adresse1 }}</td>
            </tr>
            <tr>
                <th>Adresse2</th>
                <td>{{ entity.adresse2 }}</td>
            </tr>
            <tr>
                <th>Codegestionconcession</th>
                <td>{{ entity.codeGestionConcession }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('concessionairearchive') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('concessionairearchive_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
